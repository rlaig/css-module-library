{
  "name": "css-module-library",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "files": [
    "dist",
    "docs-dist",
    "style-dist"
  ],
  "sideEffects": false,
  "type": "module",
  "module": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "gendocs": "rollup -c rollup.gendocs.config.js && node gendocs-server.cjs",
    "build": "rollup -c && tcm dist",
    "build:vite": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:rc": "standard-version --prerelease rc",
    "styleguide": "node ./node_modules/.bin/nucleus --config config.nucleus.json"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "connect": "^3.7.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "mdcss": "^1.5.2",
    "mdcss-theme-github": "^2.4.2",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "sass": "^1.56.1",
    "serve-static": "^1.15.0",
    "standard-version": "^9.5.0",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
